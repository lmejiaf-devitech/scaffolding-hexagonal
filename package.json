{
  "name": "terpel_pos_app_terpel",
  "version": "1.0.0",
  "description": "Scaffolding hexagonal base",
  "main": "dist/presentation/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/presentation/index.ts",
    "ts-compile": "webpack --config webpack.config.ts",
    "start": "tsc & node .",
    "clean:pkg": "rimraf pkg",
    "clean:build": "rimraf build",
    "clean:dist": "rimraf dist",
    "clean:all": "npm-run-all clean:pkg clean:build clean:dist",
    "build:pkg-linux": "pkg package.json --compress GZip --targets node18-linux-x64 --output ./pkg/terpel_pos_app_terpel",
    "build:pkg-win": "pkg package.json --compress GZip --targets node18-win-x64 --output ./pkg/terpel_pos_app_terpel.exe",
    "compile:linux": "npm-run-all ts-compile build:pkg-linux",
    "compile:win": "npm-run-all ts-compile build:pkg-win",
    "compile:linux-web": "npm-run-all clean:all compile:linux",
    "compile:win-pos": "npm-run-all clean:all compile:win"
  },
  "keywords": [],
  "author": "Devitech",
  "bin": "./build/terpel_pos_app_terpel.js",
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-win-x64"
    ],
    "scripts": "terpel_pos_app_terpel.js"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/inversify": "^2.0.33",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.1",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "^8.10.2",
    "@types/reflect-metadata": "^0.1.0",
    "@types/webpack-node-externals": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.59.9",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "ts-loader": "^9.4.4",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^2.3.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "inversify": "^6.0.1",
    "joi": "^17.9.2",
    "log4js": "^6.9.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.9",
    "pg": "^8.11.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1"
  }
}
